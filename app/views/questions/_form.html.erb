<ul style="list-style: none;">
  <%= form_for @question do |f| %>
    <li>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </li>
    <br>
    <li>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </li>
    <li>
      <%= f.submit "Ask" %>
    </li>
  <% end %>
</ul>
